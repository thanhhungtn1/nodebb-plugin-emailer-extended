{
    "id": "nodebb-plugin-emailer-extened",
    "url": "https://github.com/thanhhungtn1/nodebb-plugin-emailer-extened",
    "name": "Emailer (Extened)",
    "description": "Allows NodeBB to send emails via SMTP and well-known email service",
    "library": "./library.js",
    "templates": "./templates",
    "hooks": [
        { "hook": "action:email.send", "method": "send" },
        { "hook": "filter:admin.header.build", "method": "admin.menu" },
        { "hook": "static:app.load", "method": "init"}
    ],
    "compatibility": "~0.5.0"
}